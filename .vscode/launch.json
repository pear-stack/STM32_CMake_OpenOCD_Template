{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Microcontroller - STLink-V3",
      "cwd": "${workspaceFolder}",
      "type": "cortex-debug",
      "executable": "${command:cmake.launchTargetPath}",
      "request": "launch",
      "servertype": "stlink",
      "device": "STM32F303RE",
      "interface": "swd",
      "serialNumber": "",
      "runToEntryPoint": "main",
      "svdFile": "STM32F303.svd",
      "v1": false,
      "showDevDebugOutput": "both"
    },
    {
      "name": "openocd",
      "request": "launch",
      "type": "cortex-debug",
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "Build project",
      "servertype": "openocd",
      "showDevDebugOutput": "raw",
      "executable": "${command:cmake.launchTargetPath}",
      "device": "STM32F303RE",
      "svdFile": "STM32F303.svd",
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f3x.cfg"
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/smuff/Desktop/stm32_cmake_template",
      "program": "/home/smuff/Desktop/stm32_cmake_template/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}